#include <stdio.h>

struct student 
{
    char name[20];
    int roll;
    float marks;
}s;


void printer(char name[], int roll, float marks)
{
    printf("%s %d %f", s.name, s.roll, s.marks);
}


int main() {
	scanf("%s %d %f", s.name, &s.roll, &s.marks);
	printer(s.name, s.roll, s.marks);

    return 0;
}

