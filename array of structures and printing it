#include <stdio.h>

struct student 
{
    char name[20];
    int roll;
    float marks;
}s[5];                                   //array of 5 structures


int main() {
    for(int i=0; i<5; i++)               //array of 5 structures
    {
	    scanf("%s %d %f", s[i].name, &s[i].roll, &s[i].marks);
    }    
    for(int i=0; i<5; i++)               //array of 5 structures
    {
	    printf("%s %d %f \n", s[i].name, s[i].roll, s[i].marks);
    } 

    return 0;
}



// meow1 10 50
// meow2 11 51
// meow3 12 52
// meow4 13 53
// meow5 14 54 
